{
  "name": "front-end",
  "type": "module",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "build": "yarn nuxt build",
    "dev": "yarn nuxt dev",
    "generate": "yarn nuxt generate",
    "preview": "yarn nuxt preview",
    "postinstall": "yarn nuxt prepare",
    "test:pact": "yarn vitest run tests/pact",
    "test": "yarn vitest run",
    "sonar": "sonar-scanner",
    "lint": "yarn run lint:js && npm run lint:style && npm run type-check",
    "lint:fix": "yarn run lint:js:fix && npm run lint:style:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:style": "stylelint \"**/*.{css,scss,vue}\"",
    "lint:style:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/image": "2.0.0",
    "@nuxt/ui": "^4.4.0",
    "nuxt": "^4.3.1",
    "sass-embedded": "^1.97.3",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@nuxt/eslint": "^1.15.1",
    "@pact-foundation/pact": "^16.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-plugin-format": "^1.4.0",
    "happy-dom": "^20.7.0",
    "postcss-html": "^1.8.1",
    "sonarqube-scanner": "^4.3.4",
    "stylelint": "^17.3.0",
    "stylelint-config-recess-order": "^7.6.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^7.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
